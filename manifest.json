{
  "name": "Favicon API in a popup",
  "version": "1.1",
  "description": "Demonstrates favicon with 404 fallback",
  "manifest_version": 3,
  "permissions": [
    "favicon",
    "bookmarks",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; frame-src *;"
  }
}
